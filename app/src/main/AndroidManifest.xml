<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fernandobrscunha.classcontrol">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SchoolActivity"></activity>
        <activity
            android:name=".activitys.SchoolListActivity"
            android:label="@string/title_activity_school_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ClassActivity" />
        <activity android:name=".ClassRoomActivity" />
        <activity
            android:name=".ClassListActivity"
            android:label="@string/title_activity_class_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>